package javatpa;
import java.util.Scanner;
public class Ex4 {
   public static void main(String[] args) {
       Scanner in = new Scanner(System.in);
       
       final int TAM = 11;
       int i;
       int[] a = new int[TAM];
       
       for (i = 0; i <TAM; i++) {
           a[i] = 1 << i;
       }
       for (i = 0; i <TAM; i++) {
           System.out.println("A[" + i + "] = " + a[i]);
       }
   }
}

